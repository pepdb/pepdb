%h3 Edit Target 
%form{:id => "editdata", :action => "#{url '/validate-data'}", :method => "post", :target => "notify"}
  %input{:type => "hidden", :value => "target", :name => "submittype", :id =>"submittype"}
  %input{:type => "hidden", :value => params[:selElem], :name => "eleid", :id =>"eleid"}
  %input{:type => "hidden", :value => "targets", :name => "tab",:id =>"tab"}
  %fieldset.tarform
    %legend 
      target information:
    %p
      = partial :editlist, locals: {selected:@target[:species], label:"sp", fieldname:"species:", listname:"splist", listlabel:"fbsp", dbdata:@species, columnname: :species} 
    %p
      = partial :editlist, locals: {selected:@target[:tissue], label:"tis", fieldname:"tissue:", listname:"tislist", listlabel:"fbtis", dbdata:@tissues, columnname: :tissue} 
    %p
      = partial :editlist, locals: {selected:@target[:cell], label:"cell", fieldname:"cell:", listname:"celllist", listlabel:"fbcell", dbdata:@cells, columnname: :cell} 
        
  %input{:type => "submit", :value => "update target"}
  %input{:type => "button", :id => "delbut",:value => "delete entry"}
#respage
  %iframe{:name => "notify", :id =>"notify"}

